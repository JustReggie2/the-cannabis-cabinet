<h1><%= @user.username %>, welcome to your cabinet hub.</h1>
  <br>
<h3>Create a new <a href="#">cabinet</a>.</h3>
  <br>
<h3>Visit the <a href="#">Strain Library</a> to browse and/or create strains.</h3>

<div class="container">
  <% @user.cabinets.each do |cabinet| %>
    <div class="row">
      <a href="#"><%= cabinet.name %></a>
      <br>
      <% cabinet.strains.each do |strain| %>
        <a href="#"><%= strain.name %></a>
        <br>
      <% end %>
    </div>
  <% end %>    
</div>
