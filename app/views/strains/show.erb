<a href="/myaccount">Home</a>
<a href="/strains/<%= @strain.slug %>/edit">Edit</a>
<a href="/strains">Strain Library</a>
<h1><%= @strain.name %></h1>

<h2>Category: <%= @strain.category %></h2>

<h3>Positives:</h3>
<p><h3><%= @strain.positives %></h3></p>

<h3>Negatives:</h3>
<p><h3><%= @strain.negatives %></h3></p>

<form class="" action="/mycabinets/add" method="post">
  <label for="cabinet_id">Add to cabinet:</label>
    <select class="" name="cabinet_id">
      <% @user.cabinets.each do |cabinet| %>
        <option value="<%= cabinet.id %>"><%= cabinet.name %></option>
      <% end %>
    </select>
  <input type="hidden" name="strain[]" value="<%= @strain.id %>">
  <input type="submit" name="submit" value="Add Strain">
</form>
